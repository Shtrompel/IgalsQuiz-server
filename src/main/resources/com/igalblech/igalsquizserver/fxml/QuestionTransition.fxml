<VBox fx:id="vbox" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" prefHeight="800.0" style="-fx-background-color: #f3f0e2;" stylesheets="@question.css" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.igalblech.igalsquizserver.controllers.QuestionTransitionController">

   <children>
